{
  "name": "where-abouts",
  "version": "0.0.1",
  "description": "Kanban board of peopls Whereabouts",
  "productName": "Whereabouts",
  "author": "Ainsof So'o <ainsofs@sprep.org>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "fix:eslint": "eslint --ext .js,.vue src/ --fix",
    "fix:prettier": "prettier --write --loglevel warn 'src/**/*.vue'",
    "fix-code-style": "npm run fix:prettier && npm run fix:eslint"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^8.5.0",
    "prettier": "^2.5.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
